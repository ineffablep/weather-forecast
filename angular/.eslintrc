{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": [
    "angular"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "globals": {
    "require": true,
    "module": true,
    "global": true,
    "window": true,
    "describe": true,
    "it": true,
    "beforeAll": true,
    "beforeEach": true,
    "afterEach": true,
    "afterAll": true,
    "expect": true,
    "__dirname": true,
    "spyOn": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      0,
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "brace-style": [
      1,
      "1tbs"
    ],
    "comma-style": [
      1,
      "last"
    ],
    "default-case": 1,
    "func-style": [
      1,
      "declaration"
    ],
    "no-floating-decimal": 1,
    "no-nested-ternary": 1,
    "no-undefined": 1,
    "radix": 2,
    "space-before-function-paren": [
      1,
      "never"
    ],
    "space-before-blocks": 1,
    "spaced-comment": [
      1,
      "always",
      {
        "exceptions": [
          "-"
        ]
      }
    ],
    "valid-jsdoc": [
      1,
      {
        "requireReturn": false,
        "prefer": {
          "return": "returns"
        }
      }
    ],
    "wrap-iife": 1,
    "global-strict": 0,
    "no-alert": 1,
    "camelcase": 1,
    "eqeqeq": [
      1,
      "allow-null"
    ],
    "no-empty": 1,
    "no-underscore-dangle": 0,
    "no-obj-calls": 1,
    "new-cap": 1,
    "no-shadow": 1,
    "no-invalid-regexp": 1,
    "comma-dangle": [
      1,
      "never"
    ],
    "no-undef": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-new": 1,
    "no-extra-semi": 1,
    "no-debugger": 1,
    "no-caller": 1,
    "semi": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "no-unreachable": 1,
    "eol-last": 1,
    "curly": 1,
    "dot-notation": 1,
    "yoda": 1,
    "no-trailing-spaces": 1,
    "no-multi-spaces": 1,
    "no-mixed-spaces-and-tabs": [
      1,
      "smart-tabs"
    ]
  },
  "ecmaFeatures": {
    "destructuring": true,
    "spread": true,
    "blockBindings": true,
    "modules": true
  }
}