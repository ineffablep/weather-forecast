{
  "name": "angular-weather-forecast",
  "version": "1.0.0",
  "description": "Weather forecast app using Angular made with Webpack",
  "scripts": {
    "build": "webpack --config webpack.build.js --bail -p",
    "lint": "eslint js/** ",
    "start": "node node_modules/.bin/webpack-dev-server --history-api-fallback --hot --inline --progress",
